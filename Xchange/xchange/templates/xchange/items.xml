<?xml version="1.0"?>
<!DOCTYPE items [
<!ELEMENT items (item*)>
<!ELEMENT item (user,text,time)>
<!ELEMENT user (#PCDATA)>
<!ELEMENT text (#PCDATA)>
<!ELEMENT time (#PCDATA)>
]>
<items>
    {% for item in items %}
        <item>
            <id>{{item.id}}</id>
            <text>{{item.text}}</text>
            <ipAddr>{{item.ip_addr}}</ipAddr>
        </item>
    {% endfor %}
</items>
